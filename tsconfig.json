{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "typeRoots": [
      "../node_modules/@types", 
      "../@types"
      // "../alltypes.d.ts",
    ]
  },
  "include": [
    "src"
, "src/models/.customerts", "src/app/containers/Admin/Contact/components/ContactDetail/components/.lengthDetailForm"  ]
}
